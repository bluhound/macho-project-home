<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/SoftwareRatingEntity.hbm.xml,v 1.2 2008/01/28 10:30:12 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2008/01/28 10:30:12 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.SoftwareRatingEntity" table="NW_DM_SOFTWARE_RATING">
        <id name="id" type="long">
            <column name="RATING_ID" precision="20" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="softwarePackage" class="com.npower.dm.hibernate.entity.SoftwarePackageEntity" fetch="select">
            <column name="PACKAGE_ID" precision="20" scale="0" not-null="true"/>
        </many-to-one>
        <property name="name" type="string">
            <column name="NAME" not-null="true" />
        </property>
        <property name="rate" type="long">
            <column name="RATE" precision="16" scale="0" />
        </property>
    </class>
</hibernate-mapping>
