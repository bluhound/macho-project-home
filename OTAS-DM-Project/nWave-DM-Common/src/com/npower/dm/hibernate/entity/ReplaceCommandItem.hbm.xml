<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/ReplaceCommandItem.hbm.xml,v 1.2 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.ReplaceCommandItem" table="NW_DM_REPL_CMD_ITEM">
        <composite-id name="id" class="com.npower.dm.hibernate.entity.ReplaceCommandItemID">
            <key-property name="replCmdId" type="long">
                <column name="REPL_CMD_ID" precision="16" scale="0" />
            </key-property>
            <key-property name="itemIndex" type="long">
                <column name="ITEM_INDEX" precision="16" scale="0" />
            </key-property>
        </composite-id>
        <many-to-one name="nwDmDmCmd" class="com.npower.dm.hibernate.entity.DMCommand" update="false" insert="false" fetch="select">
            <column name="REPL_CMD_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <property name="target" type="string">
            <column name="TARGET" />
        </property>
        <property name="MFormat" type="string">
            <column name="M_FORMAT" />
        </property>
        <property name="MType" type="string">
            <column name="M_TYPE" />
        </property>
        <property name="itemDataKind" type="string">
            <column name="ITEM_DATA_KIND" length="20" />
        </property>
        <property name="updateId" type="string">
            <column name="UPDATE_ID" />
        </property>
        <property name="rawData" type="clob">
            <column name="RAW_DATA" />
        </property>
        <property name="binaryData" type="blob">
            <column name="BINARY_DATA" />
        </property>
    </class>
</hibernate-mapping>
