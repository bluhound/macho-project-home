<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/ModelCharacter.hbm.xml,v 1.2 2008/06/05 10:34:41 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2008/06/05 10:34:41 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.ModelCharacterEntity" table="nW_DM_MODEL_CHARACTER">
        <id name="ID" type="long" unsaved-value="0">
            <column name="MODEL_CHARACTER_ID" precision="20" scale="0" />
            <generator class="native">
              <param name="sequence">nwavedm_global_sequence</param>
            </generator>
        </id>
        <many-to-one name="model" class="com.npower.dm.hibernate.entity.ModelEntity" fetch="select">
            <column name="MODEL_ID" precision="20" scale="0" not-null="true" />
        </many-to-one>
        <property name="category" type="string">
            <column name="CATEGORY" not-null="false"/>
        </property>
        <property name="name" type="string">
            <column name="NAME" not-null="true"/>
        </property>
        <property name="value" type="string">
            <column name="VALUE" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>
