<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/DMPackageSender.hbm.xml,v 1.2 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.DMPackageSender" table="NW_DM_DM_PKG_SENDER">
        <id name="dmPkgSenderId" type="long" unsaved-value="0">
            <column name="DM_PKG_SENDER_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="nwDmDmCmdPkg" class="com.npower.dm.hibernate.entity.DMCommandPackage" fetch="select">
            <column name="COMMAND_PACKAGE_ID" precision="16" scale="0" />
        </many-to-one>
        <property name="nextCommandIndex" type="long">
            <column name="NEXT_COMMAND_INDEX" precision="16" scale="0" />
        </property>
        <property name="nextItemIndex" type="long">
            <column name="NEXT_ITEM_INDEX" precision="16" scale="0" />
        </property>
        <property name="chunkIndex" type="long">
            <column name="CHUNK_INDEX" precision="16" scale="0" />
        </property>
        <set name="nwDmDmPkgSenderCmdIdses" inverse="true">
            <key>
                <column name="DM_PKG_SENDER_ID" precision="16" scale="0" not-null="true" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.DMPkgSenderCMDIDs" />
        </set>
        <set name="nwDmDmPkgHandlers" inverse="true">
            <key>
                <column name="PACKAGE_SENDER_ID" precision="16" scale="0" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.DMPackageHandler" />
        </set>
        <set name="nwDmDmPkgRespProcs" inverse="true">
            <key>
                <column name="PACKAGE_SENDER_ID" precision="16" scale="0" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.DMPkgRespProc" />
        </set>
    </class>
</hibernate-mapping>
