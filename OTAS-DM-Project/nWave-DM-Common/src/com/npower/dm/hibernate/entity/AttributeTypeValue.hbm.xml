<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/AttributeTypeValue.hbm.xml,v 1.3 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.3 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.AttributeTypeValueEntity" table="NW_DM_ATTRIB_TYPE_VALUES">
        <id name="ID" type="long" unsaved-value="0">
            <column name="ATTRIBUTE_TYPE_VALUE_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="profileAttribType" class="com.npower.dm.hibernate.entity.ProfileAttributeTypeEntity" fetch="select">
            <column name="ATTRIBUTE_TYPE_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <property name="value" type="string">
            <column name="VALUE" not-null="true" unique="false" />
        </property>
    </class>
</hibernate-mapping>
