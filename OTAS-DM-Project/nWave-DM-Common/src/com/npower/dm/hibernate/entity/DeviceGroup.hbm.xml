<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/DeviceGroup.hbm.xml,v 1.3 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.3 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.DeviceGroupEntity" table="NW_DM_DEVICE_GROUP">
        <id name="ID" type="long" unsaved-value="0">
            <column name="DEVICE_GROUP_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <property name="changeVersion" type="long">
            <column name="CHANGE_VERSION" precision="16" scale="0" not-null="true" />
        </property>
        <set name="devicesForDeviceGroup" inverse="true">
            <key>
                <column name="DEVICE_GROUP_ID" precision="16" scale="0" not-null="true" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.Device4DeviceGroup" />
        </set>
        <set name="devicesForInitialGroup" inverse="true">
            <key>
                <column name="INITIAL_GROUP_ID" precision="16" scale="0" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.Device4DeviceGroup" />
        </set>
        <set name="prElements" inverse="true">
            <key>
                <column name="DEVICE_GROUP_ID" precision="16" scale="0" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.Element4Provision" />
        </set>
    </class>
</hibernate-mapping>
