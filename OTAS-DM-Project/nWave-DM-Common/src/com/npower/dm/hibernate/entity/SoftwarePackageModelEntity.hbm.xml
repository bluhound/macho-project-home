<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/SoftwarePackageModelEntity.hbm.xml,v 1.3 2008/09/04 10:49:59 zhao Exp $
  * $Revision: 1.3 $
  * $Date: 2008/09/04 10:49:59 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.SoftwarePackageModelEntity" table="nW_DM_SOFTWARE_PACKAGE_MODEL">
        <id name="id" type="long">
            <column name="PACKAGE_MODEL_ID" precision="20" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="softwarePackage" class="com.npower.dm.hibernate.entity.SoftwarePackageEntity" fetch="select">
            <column name="PACKAGE_ID" precision="20" scale="0" not-null="true"/>
        </many-to-one>
        <many-to-one name="model" class="com.npower.dm.hibernate.entity.ModelEntity" fetch="select">
            <column name="MODEL_ID" precision="20" scale="0" not-null="false"/>
        </many-to-one>
        <many-to-one name="modelClassification" class="com.npower.dm.hibernate.entity.ModelClassificationEntity" fetch="select">
            <column name="MODEL_CLASSIFICATION_ID" precision="20" scale="0" not-null="false"/>
        </many-to-one>
        <property name="modelFamilyExtID" type="string">
            <column name="MODEL_FAMILY" length="255" />
        </property>
        <property name="dynamicModelSelector" type="string">
            <column name="DYNAMIC_MODEL_SELECTOR" />
        </property>
    </class>
</hibernate-mapping>
