<ProfileMetaData>
    <AttributeTypes>
        <!-- Using by MMS Template -->
    </AttributeTypes>

    <ProfileTemplates>
        <!-- Default MMS Template -->
        <ProfileTemplate>
            <CategoryName>MMS</CategoryName>
            <Name>MMS Default Template</Name>
            <NeedsNAP>false</NeedsNAP>
            <NeedsProxy>true</NeedsProxy>
            <Attributes>
                <Attribute>
                    <Name>Display Name</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                </Attribute>
                <Attribute>
                    <Name>MMSC Server</Name>
                    <TypeName>String</TypeName>
                    <DefaultValue>http://mmsc.service.com</DefaultValue>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                </Attribute>
                <!-- APLink mode for Nokia phones -->
                <Attribute>
                    <Name>APLink</Name>
                    <TypeName>APLink</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                </Attribute>
               <!-- APName mode for Motorola phones -->
                <Attribute>
                    <Name>APName</Name>
                    <TypeName>APName</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                </Attribute>
                <!-- SelfLink to active configuration for SonyEricsson phones -->
                <Attribute>
                    <Name>SelfLink</Name>
                    <TypeName>SelfLink</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                </Attribute>
            </Attributes>
        </ProfileTemplate>

    </ProfileTemplates>
</ProfileMetaData>
