<ProfileMetaData>
    <AttributeTypes>
        <!-- Using by Proxy Template -->
        <AttributeType>
        	<Name>PROXY PPGAUTH-TYPE</Name>
        	<Description>The PPGAUTH-TYPE parameter links the PROXY-ID and PROXY-PW to an authentication method.</Description>
        	<Values>
        		<Value>HTTP-BASIC</Value>
        		<Value>HTTP-DIGEST</Value>
        	</Values>
        </AttributeType>

        <AttributeType>
        	<Name>PROXY PXADDRTYPE</Name>
        	<Description>The PXADDRTYPE indicates the format and the interpretation of the PXADDR attribute.</Description>
        	<Values>
        		<Value>IPV4</Value>
        		<Value>IPV6</Value>
        		<Value>E164</Value>
        		<Value>ALPHA</Value>
        	</Values>
        </AttributeType>
        <AttributeType>
        	<Name>PROXY PXAUTH-TYPE</Name>
        	<Description>The PXAUTH-TYPE indicates the proxy authentication method: HTTP proxy authentication or WTLS methods.</Description>
        	<Values>
        		<Value>HTTP-BASIC</Value>
        		<Value>HTTP-DIGEST</Value>
        		<Value>WTLS-SS</Value>
        	</Values>
        </AttributeType>
        <AttributeType>
        	<Name>PXLOGICAL SERVICE</Name>
        	<Description>The SERVICE parameter specifies which service is available behind this particular port number.</Description>
        	<Values>
        		<Value>CL-WSP</Value>
        		<Value>CO-WSP</Value>
        		<Value>CL-SEC-WSP</Value>
        		<Value>CO-SEC-WSP</Value>
        		<Value>CO-SEC-WTA</Value>
        		<Value>CL-SEC-WTA</Value>
        		<Value>OTA-HTTP-TO</Value>
        		<Value>OTA-HTTP-TLS-TO</Value>
        		<Value>OTA-HTTP-PO</Value>
        		<Value>OTA-HTTP-TLS-PO</Value>
        	</Values>
        </AttributeType>
        <AttributeType>
        	<Name>PXPHYSICAL SERVICE</Name>
        	<Description>The SERVICE parameter specifies which service is available behind this particular port number.</Description>
        	<Values>
        		<Value>CL-WSP</Value>
        		<Value>CO-WSP</Value>
        		<Value>CL-SEC-WSP</Value>
        		<Value>CO-SEC-WSP</Value>
        		<Value>CO-SEC-WTA</Value>
        		<Value>CL-SEC-WTA</Value>
        		<Value>OTA-HTTP-TO</Value>
        		<Value>OTA-HTTP-TLS-TO</Value>
        		<Value>OTA-HTTP-PO</Value>
        		<Value>OTA-HTTP-TLS-PO</Value>
        	</Values>
        </AttributeType>
    </AttributeTypes>

    <ProfileTemplates>
        <!-- Default NAP Template defined from OMA CP NAP Profile -->
        <ProfileTemplate>
            <CategoryName>Proxy</CategoryName>
            <Name>Proxy Default Template</Name>
            <NeedsNAP>true</NeedsNAP>
            <NeedsProxy>false</NeedsProxy>
            <Attributes>
                <Attribute>
                    <Name>NAME</Name>
                    <DisplayName>Name</DisplayName>
                    <TypeName>String</TypeName>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The NAME indicates a logical, user readable, identity (property) of the configuration element.</Description>
                </Attribute>
                <Attribute>
                    <Name>STARTPAGE</Name>
                    <DisplayName>Startpage</DisplayName>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The STARTPAGE parameter MUST be an absolute URI [RFC2396] and defines the homepage or start page
associated with the services accessible from the proxy.</Description>
                </Attribute>
                <Attribute>
                    <Name>PXADDRTYPE</Name>
                    <DisplayName>Proxy Addr Type</DisplayName>
                    <TypeName>PROXY PXADDRTYPE</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <DefaultValue>IPV4</DefaultValue>
                    <Description>The PXADDRTYPE indicates the format and the interpretation of the PXADDR attribute. The PXADDRTYPE can
indicate an IP address, a phone number according to the E164 scheme, or a generic alphanumeric address format. If
the parameter is not present or if no value is given, the default value IPV4 should be assumed.</Description>
                </Attribute>
                <Attribute>
                    <Name>PXADDR</Name>
                    <DisplayName>Proxy Address</DisplayName>
                    <TypeName>String</TypeName>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The PXADDR can store addresses of different kinds, for example an IP address or a SME number. The type of
address in the field is defined by the PXADDRTYPE parameter.</Description>
                </Attribute>
                <Attribute>
                    <Name>PXPHYSICAL PORTNBR</Name>
                    <DisplayName>Proxy Port</DisplayName>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <DefaultValue>8080</DefaultValue>
                    <Description>The PORTNBR parameter contains the value of the port number. The port number must be given as a decimal
number and must fit within the range of a 16 bit unsigned integer.</Description>
                </Attribute>
                <Attribute>
                    <Name>PXAUTH-TYPE</Name>
                    <DisplayName>Proxy Auth Type</DisplayName>
                    <TypeName>PROXY PXAUTH-TYPE</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The PXAUTH-TYPE indicates the proxy authentication method: HTTP proxy authentication or WTLS methods.</Description>
                </Attribute>
                <Attribute>
                    <Name>PXAUTH-ID</Name>
                    <DisplayName>Proxy Auth Name</DisplayName>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The PXAUTH-ID indicates the proxy authentication identifier.</Description>
                </Attribute>
                <Attribute>
                    <Name>PXAUTH-PW</Name>
                    <DisplayName>Proxy Auth Passwd</DisplayName>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The PXAUTH-PW indicates the proxy authentication secret. The usage of the parameter is defined by the
PXAUTH-TYPE.</Description>
                </Attribute>
                <Attribute>
                    <Name>PXPHYSICAL DOMAIN</Name>
                    <DisplayName>Proxy Domain</DisplayName>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>true</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The DOMAIN parameter indicates a domain, for which the proxy is responsible. The proxy might support multiple
domains.</Description>
                </Attribute>
                <Attribute>
                    <Name>TO-NAPID</Name>
                    <DisplayName></DisplayName>
                    <TypeName>APLink</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>This parameter refers to a network access point with a matching NAPID parameter. It is only possible to refer to
network access points defined within the same provisioning document.</Description>
                </Attribute>
                
                <!-- PXLOGICAL DOMAIN and PXPHYSICAL SERVICE required by Motorola -->
                <Attribute>
                    <Name>PXLOGICAL DOMAIN</Name>
                    <DisplayName>Proxy Domain</DisplayName>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>true</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The DOMAIN parameter indicates a domain, for which the proxy is responsible.</Description>
                </Attribute>
                <Attribute>
                    <Name>PXPHYSICAL SERVICE</Name>
                    <DisplayName>Proxy Service</DisplayName>
                    <TypeName>PXPHYSICAL SERVICE</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>true</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <Description>The SERVICE parameter specifies which service is available behind this particular port number.</Description>
                </Attribute>
                
                <!-- Unused Attribute -->
