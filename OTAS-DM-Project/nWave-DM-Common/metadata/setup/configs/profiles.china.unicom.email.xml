  <!-- Email Profile: GMail-->
  <Profile>
    <ProfileName>GMail</ProfileName>
    <Template>Email Default Template</Template>
    <Carrier>ChinaUnicom</Carrier>
    <NAPProfile>UNINET</NAPProfile>
    <ProxyProfile></ProxyProfile>
    <IsUserProfile>true</IsUserProfile>
    <Description>Google Email服务, 该服务的详细情况请访问GMail服务提供商的网站http://www.gmail.com</Description>
    <Attributes>
      <Attribute>
         <Name>Display Name</Name>
         <Value>GMail</Value>
      </Attribute>
      <Attribute>
         <Name>Username</Name>
         <Value>@gmail.com</Value>
      </Attribute>    
      <Attribute>
         <Name>Email Address</Name>
         <Value>@gmail.com</Value>
      </Attribute>    
      <Attribute>
         <Name>Reply To Address</Name>
         <Value></Value>
      </Attribute>
      <Attribute>
         <Name>Receiving Server Address</Name>
         <Value>pop.gmail.com</Value>
      </Attribute>
      <Attribute>
         <Name>Receiving Server Port</Name>
         <Value>995</Value>
      </Attribute>
      <Attribute>
         <Name>Use SSL Receiving Service</Name>
         <Value>true</Value>
      </Attribute>
      <Attribute>
         <Name>Sending Server Address</Name>
         <Value>smtp.gmail.com</Value>
      </Attribute>
      <Attribute>
         <Name>Sending Server Port</Name>
         <Value>465</Value>
      </Attribute>
      <Attribute>
         <Name>Use SSL Sending Service</Name>
         <Value>true</Value>
      </Attribute>
      <Attribute>
         <Name>Use SMTP authentication</Name>
         <Value>true</Value>
      </Attribute>
      <Attribute>
         <Name>Mailbox Protocol</Name>
         <Value>POP</Value>
      </Attribute>
    </Attributes>
  </Profile>
  
