<ProfileMetaData>

    <ProfileMappings>
    
        <!-- Default NAP Template Mapping      -->
        <ProfileMapping>
            <ManufacturerExternalId>SonyEricsson</ManufacturerExternalId>
            <ModelName>M600i</ModelName>
            <TemplateName>NAP Default Template</TemplateName>
            <RootNodePath>./NAP/${NodeName}</RootNodePath>
            <ShareRootNode>false</ShareRootNode>
            <NodeMappings>
                <ValueMapping>
                    <Name>NapId</Name>
                    <Value>100</Value>
                    <NodeRelativePath>NapId</NodeRelativePath>
                </ValueMapping>
                <AttributeMapping>
                    <AttributeName>NAME</AttributeName>
                    <NodeRelativePath>Name</NodeRelativePath>
                </AttributeMapping>
                <ValueMapping>
                    <Name>AuthType</Name>
                    <Value>PAP</Value>
                    <NodeRelativePath>AuthInfo/AuthType</NodeRelativePath>
                </ValueMapping>
                <AttributeMapping>
                    <AttributeName>AUTHNAME</AttributeName>
                    <NodeRelativePath>AuthInfo/AuthName</NodeRelativePath>
                </AttributeMapping>
                <AttributeMapping>
                    <AttributeName>AUTHSECRET</AttributeName>
                    <NodeRelativePath>AuthInfo/AuthSecret</NodeRelativePath>
                </AttributeMapping>
                <AttributeMapping>
                    <AttributeName>NAP-ADDRTYPE</AttributeName>
                    <NodeRelativePath>AddrType</NodeRelativePath>
                </AttributeMapping>
                <AttributeMapping>
                    <AttributeName>NAP-ADDRESS</AttributeName>
                    <NodeRelativePath>Addr</NodeRelativePath>
                </AttributeMapping>
                <ValueMapping>
                    <Name>Linger</Name>
                    <Value>900</Value>
                    <NodeRelativePath>Linger</NodeRelativePath>
                </ValueMapping>
            </NodeMappings>
            <CanAssignToDevice>true</CanAssignToDevice>
        </ProfileMapping>

    </ProfileMappings>

</ProfileMetaData>
