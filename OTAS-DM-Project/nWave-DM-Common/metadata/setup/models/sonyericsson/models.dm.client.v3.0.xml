    <!-- OMA DM Stage -->
    <IsOmaDmEnabled>true</IsOmaDmEnabled>
    <OmaDmVersion>1.2</OmaDmVersion>
    
    <!-- OMA CP Stage -->
    <IsOmaCpEnabled>true</IsOmaCpEnabled>
    <OmaCpVersion>1.1</OmaCpVersion>
    
    <!-- Nokia OTA Stage -->
