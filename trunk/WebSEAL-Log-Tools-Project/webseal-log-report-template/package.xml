<?xml version="1.0" encoding="UTF-8"?>
<assembly>
  <id>bin</id>
  <formats>
    <format>war</format>
  </formats>
  <includeBaseDirectory>true</includeBaseDirectory>
  <baseDirectory>/</baseDirectory>
  <fileSets>
    <!-- Pack Runtime -->
    <fileSet>
      <directory>/app_login</directory>
      <outputDirectory>/app_login</outputDirectory>
      <includes>
        <include>**</include>
      </includes>
      <fileMode>644</fileMode>
    </fileSet>
    <fileSet>
      <directory>/js</directory>
      <outputDirectory>/js</outputDirectory>
      <includes>
        <include>**</include>
      </includes>
      <fileMode>644</fileMode>
    </fileSet>
    <fileSet>
      <directory>/sql</directory>
      <outputDirectory>/sql</outputDirectory>
      <includes>
        <include>**</include>
      </includes>
      <fileMode>644</fileMode>
    </fileSet>
    <fileSet>
      <directory>/css</directory>
      <outputDirectory>/css</outputDirectory>
      <includes>
        <include>**</include>
      </includes>
      <fileMode>644</fileMode>
    </fileSet>
    <fileSet>
      <directory>/sgm</directory>
      <outputDirectory>/sgm</outputDirectory>
      <includes>
        <include>**</include>
      </includes>
      <fileMode>644</fileMode>
    </fileSet>
    <fileSet>
      <directory>/</directory>
      <outputDirectory>/</outputDirectory>
      <includes>
        <include>*.jsp</include>
        <include>*.rptlibrary</include>
      </includes>
      <fileMode>644</fileMode>
    </fileSet>
  </fileSets>
  <dependencySets>
    <!-- Pack WAR -->
    <dependencySet>
      <outputDirectory>/</outputDirectory>
      <unpack>true</unpack>
      <includes>
        <include>*webseal-log-report-runtime:war</include>
      </includes>
    </dependencySet>
  </dependencySets>
</assembly>
