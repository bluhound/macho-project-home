<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/Steps.hbm.xml,v 1.2 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.Steps" table="NW_DM_STEPS">
        <id name="stepsId" type="long" unsaved-value="0">
            <column name="STEPS_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="nwDmUpdateWorkflow" class="com.npower.dm.hibernate.entity.UpdateWorkflow" fetch="select">
            <column name="WORKFLOW_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <property name="name" type="string">
            <column name="NAME" length="100" not-null="true" />
        </property>
        <property name="stepId" type="long">
            <column name="STEP_ID" precision="16" scale="0" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
