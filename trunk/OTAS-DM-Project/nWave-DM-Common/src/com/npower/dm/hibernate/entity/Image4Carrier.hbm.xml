<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/Image4Carrier.hbm.xml,v 1.2 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.Image4Carrier" table="NW_DM_IMAGE_CARRIERS">
        <id name="imageCarriersId" type="long" unsaved-value="0">
            <column name="IMAGE_CARRIERS_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="nwDmImage" class="com.npower.dm.hibernate.entity.ImageEntity" fetch="select">
            <column name="IMAGE_ID" precision="16" scale="0" />
        </many-to-one>
        <many-to-one name="nwDmImageUpdateStatus" class="com.npower.dm.hibernate.entity.ImageUpdateStatusEntity" fetch="select">
            <column name="STATUS_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <many-to-one name="nwDmCarrier" class="com.npower.dm.hibernate.entity.CarrierEntity" fetch="select">
            <column name="CARRIER_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <property name="changeVersion" type="long">
            <column name="CHANGE_VERSION" precision="16" scale="0" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
