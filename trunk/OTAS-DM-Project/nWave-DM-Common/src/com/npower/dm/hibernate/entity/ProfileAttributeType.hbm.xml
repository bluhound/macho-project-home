<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/ProfileAttributeType.hbm.xml,v 1.5 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.5 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.ProfileAttributeTypeEntity" table="NW_DM_PROFILE_ATTRIB_TYPE">
        <id name="ID" type="long" unsaved-value="0">
            <column name="ATTRIBUTE_TYPE_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <property name="name" type="string">
            <column name="NAME" not-null="true" unique="true" />
        </property>
        <property name="description" type="string">
            <column name="DESCRIPTION" />
        </property>
        <property name="lastUpdatedBy" type="string">
            <column name="LAST_UPDATED_BY" length="30" />
        </property>
        <property name="lastUpdatedTime" type="java.util.Date">
            <column name="LAST_UPDATED_TIME" length="7" />
        </property>
        <property name="changeVersion" type="long">
            <column name="CHANGE_VERSION" precision="16" scale="0" not-null="true" />
        </property>
        <set name="profileAttributes" inverse="true">
            <key>
                <column name="ATTRIBUTE_TYPE_ID" precision="16" scale="0" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.ProfileAttributeEntity" />
        </set>
        <set name="attribTypeValues" inverse="true" order-by="ATTRIBUTE_TYPE_VALUE_ID asc">
            <key>
                <column name="ATTRIBUTE_TYPE_ID" precision="16" scale="0" not-null="true" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.AttributeTypeValueEntity" />
        </set>
    </class>
</hibernate-mapping>
