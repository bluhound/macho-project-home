<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/ProfileAttributeValue.hbm.xml,v 1.3 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.3 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.ProfileAttributeValueEntity" table="NW_DM_PROFILE_ATTRIB_VALUE">
        <id name="ID" type="long" unsaved-value="0">
            <column name="ATTRIBUTE_VALUE_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="profileAttribute" class="com.npower.dm.hibernate.entity.ProfileAttributeEntity" fetch="select">
            <column name="ATTRIBUTE_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <property name="isMultiValued" type="boolean">
            <column name="IS_MULTI_VALUED" precision="1" scale="0" not-null="true" />
        </property>
        <property name="itemDataKind" type="string">
            <column name="ITEM_DATA_KIND" length="20" />
        </property>
        <property name="updateId" type="string">
            <column name="UPDATE_ID" />
        </property>
        <property name="rawData" type="clob">
            <column name="RAW_DATA" />
        </property>
        <property name="binaryData" type="blob">
            <column name="BINARY_DATA" />
        </property>
        <property name="MFormat" type="string">
            <column name="M_FORMAT" />
        </property>
        <property name="MType" type="string">
            <column name="M_TYPE" />
        </property>
        <set name="profileValueMaps" inverse="true">
            <key>
                <column name="ATTRIBUTE_VALUE_ID" precision="16" scale="0" not-null="true" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.ProfileValueMap" />
        </set>
        <set name="profileAssignValues" inverse="true">
            <key>
                <column name="ATTRIBUTE_VALUE_ID" precision="16" scale="0" not-null="true" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.ProfileAssignmentValue" />
        </set>
        <set name="profileValueItems" inverse="true">
            <key>
                <column name="PROFILE_ATTRIBUTE_VALUE_ID" precision="16" scale="0" not-null="true" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.ProfileValueItem" />
        </set>
    </class>
</hibernate-mapping>
