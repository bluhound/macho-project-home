<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/PrevPkgResp.hbm.xml,v 1.2 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.PrevPkgResp" table="NW_DM_PREV_PKG_RESP">
        <id name="prevPkgRespId" type="long" unsaved-value="0">
            <column name="PREV_PKG_RESP_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="nwDmDmSession" class="com.npower.dm.hibernate.entity.DMSession" fetch="select">
            <column name="DM_SESSION_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <property name="responseData" type="blob">
            <column name="RESPONSE_DATA" not-null="true" />
        </property>
        <property name="hmacHeader" type="string">
            <column name="HMAC_HEADER" />
        </property>
    </class>
</hibernate-mapping>
