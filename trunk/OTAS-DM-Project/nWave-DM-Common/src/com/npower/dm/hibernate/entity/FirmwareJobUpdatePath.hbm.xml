<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/FirmwareJobUpdatePath.hbm.xml,v 1.2 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.FirmwareJobUpdatePath" table="NW_DM_FW_JOB_UPDATE_PATH">
        <composite-id name="id" class="com.npower.dm.hibernate.entity.FirmwareJobUpdatePathID">
            <key-property name="fwJobStateId" type="long">
                <column name="FW_JOB_STATE_ID" precision="16" scale="0" />
            </key-property>
            <key-property name="updateId" type="long">
                <column name="UPDATE_ID" precision="16" scale="0" />
            </key-property>
            <key-property name="pathIndex" type="long">
                <column name="PATH_INDEX" precision="16" scale="0" />
            </key-property>
        </composite-id>
        <many-to-one name="nwDmUpdate" class="com.npower.dm.hibernate.entity.UpdateEntity" update="false" insert="false" fetch="select">
            <column name="UPDATE_ID" precision="16" scale="0" />
        </many-to-one>
        <many-to-one name="nwDmJobState" class="com.npower.dm.hibernate.entity.JobState" update="false" insert="false" fetch="select">
            <column name="FW_JOB_STATE_ID" precision="16" scale="0" />
        </many-to-one>
    </class>
</hibernate-mapping>
