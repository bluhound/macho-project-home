<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/ProfileAssignmentValue.hbm.xml,v 1.2 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.ProfileAssignmentValue" table="NW_DM_PROFILE_ASSIGN_VALUE">
        <composite-id name="ID" class="com.npower.dm.hibernate.entity.ProfileAssignmentValueID">
            <key-property name="profileAssignmentId" type="long">
                <column name="PROFILE_ASSIGNMENT_ID" precision="16" scale="0" />
            </key-property>
            <key-property name="attributeValueId" type="long">
                <column name="ATTRIBUTE_VALUE_ID" precision="16" scale="0" />
            </key-property>
        </composite-id>
        <many-to-one name="profileAttribValue" class="com.npower.dm.hibernate.entity.ProfileAttributeValueEntity" update="false" insert="false" fetch="select">
            <column name="ATTRIBUTE_VALUE_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <many-to-one name="profileAssignment" class="com.npower.dm.hibernate.entity.ProfileAssignmentEntity" update="false" insert="false" fetch="select">
            <column name="PROFILE_ASSIGNMENT_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>
