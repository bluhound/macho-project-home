<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/DeviceTree.hbm.xml,v 1.6 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.6 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.DeviceTreeEntity" table="NW_DM_DTREE">
        <id name="ID" type="long" unsaved-value="0">
            <column name="DEVICE_TREE_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <many-to-one name="rootNode" class="com.npower.dm.hibernate.entity.DeviceTreeNodeEntity" fetch="select">
            <column name="ROOT_NODE_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <property name="changeVersion" type="long">
            <column name="CHANGE_VERSION" precision="16" scale="0" not-null="true" />
        </property>
        <set name="devices" inverse="true">
            <key>
                <column name="DEVICE_TREE_ID" precision="16" scale="0" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.DeviceEntity" />
        </set>
    </class>
</hibernate-mapping>
