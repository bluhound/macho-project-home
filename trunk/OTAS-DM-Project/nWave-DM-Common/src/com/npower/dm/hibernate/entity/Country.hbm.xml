<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/Country.hbm.xml,v 1.3 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.3 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.CountryEntity" table="NW_DM_COUNTRY">
        <id name="ID" type="long" unsaved-value="0">
            <column name="COUNTRY_ID" precision="16" scale="0" />
            <generator class="native"><param name="sequence">nwavedm_global_sequence</param></generator>
        </id>
        <property name="ISOCode" type="string">
            <column name="ISO_CODE" length="2" not-null="true" />
        </property>
        <property name="countryCode" type="string">
            <column name="COUNTRY_CODE" length="20" not-null="true" />
        </property>
        <property name="name" type="string">
            <column name="COUNTRY_NAME" length="100" not-null="true" />
        </property>
        <property name="trunkCode" type="string">
            <column name="TRUNK_CODE" length="5" />
        </property>
        <property name="displayCountryCode" type="boolean">
            <column name="DISPLAY_COUNTRY_CODE" precision="1" scale="0" not-null="true" />
        </property>
        <property name="displayTrunkCode" type="boolean">
            <column name="DISPLAY_TRUNK_CODE" precision="1" scale="0" not-null="true" />
        </property>
        <property name="displayPrefix" type="boolean">
            <column name="DISPLAY_PREFIX" precision="1" scale="0" not-null="true" />
        </property>
        <property name="changeVersion" type="long">
            <column name="CHANGE_VERSION" precision="16" scale="0" not-null="true" />
        </property>
        <set name="carriers" inverse="true">
            <key>
                <column name="COUNTRY_ID" precision="16" scale="0" not-null="true" />
            </key>
            <one-to-many class="com.npower.dm.hibernate.entity.CarrierEntity" />
        </set>
    </class>
</hibernate-mapping>
