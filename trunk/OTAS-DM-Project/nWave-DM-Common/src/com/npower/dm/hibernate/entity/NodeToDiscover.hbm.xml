<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
  * $Header: /home/master/nWave-DM-Common/src/com/npower/dm/hibernate/entity/NodeToDiscover.hbm.xml,v 1.2 2006/12/26 03:15:19 zhao Exp $
  * $Revision: 1.2 $
  * $Date: 2006/12/26 03:15:19 $
-->
<hibernate-mapping>
    <class name="com.npower.dm.hibernate.entity.NodeToDiscover" table="NW_DM_NODES_TO_DISCOVER">
        <composite-id name="id" class="com.npower.dm.hibernate.entity.NodeToDiscoverID">
            <key-property name="devProvReqId" type="long">
                <column name="DEV_PROV_REQ_ID" precision="16" scale="0" />
            </key-property>
            <key-property name="nodeIndex" type="long">
                <column name="NODE_INDEX" precision="16" scale="0" />
            </key-property>
        </composite-id>
        <many-to-one name="nwDmJobState" class="com.npower.dm.hibernate.entity.JobState" update="false" insert="false" fetch="select">
            <column name="DEV_PROV_REQ_ID" precision="16" scale="0" not-null="true" />
        </many-to-one>
        <property name="nodeName" type="string">
            <column name="NODE_NAME" length="300" />
        </property>
    </class>
</hibernate-mapping>
