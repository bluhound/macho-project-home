    <!-- DDF Stage -->
    <DDFs>
      <DDF filename="ddf/sonyericsson/sonyericsson_ddf_3.1.xml" />
    </DDFs>
    
    <!-- OMA DM Profile Mappings -->
    <ProfileMappings>
