    </ProfileMappings>
    
    <!-- OTA Templates -->
    <CPTemplates filename="./models/sonyericsson/ota.templates.dm.client.3.0.xml" />

    <!-- Specifications -->
    <Specifications>
      <Specification category="general" name="operating.system" value="SonyEricsson OS"/>
      <Specification category="general" name="developer.platform" value="DM Client Version 3.1"/>
    </Specifications>
