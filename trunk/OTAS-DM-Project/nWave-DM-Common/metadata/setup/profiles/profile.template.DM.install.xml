<ProfileMetaData>
    <AttributeTypes>
        <!-- Using by DM Template -->
    </AttributeTypes>

    <ProfileTemplates>
        <!-- Default DM Template -->
        <ProfileTemplate>
            <CategoryName>DM</CategoryName>
            <Name>DM Default Template</Name>
            <NeedsNAP>true</NeedsNAP>
            <NeedsProxy>false</NeedsProxy>
            <Attributes>
                <Attribute>
                    <Name>AppID</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <DefaultValue>w7</DefaultValue>
                    <Description>This node specifies the application ID for device management account object</Description>
                </Attribute>
                <Attribute>
                    <Name>Name</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>This node specifies user displayable name for the management server.</Description>
                </Attribute>
                <Attribute>
                    <Name>ServerAddr</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>This node specifies a Management Server address</Description>
                </Attribute>
                <Attribute>
                    <Name>ServerId</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>Server ID</Description>
                </Attribute>
                <Attribute>
                    <Name>ServerPW</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>Server Password</Description>
                </Attribute>
                <Attribute>
                    <Name>ServerNonce</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>Server Nonce</Description>
                </Attribute>
                <Attribute>
                    <Name>ClientId</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>Client ID</Description>
                </Attribute>
                <Attribute>
                    <Name>ClientPW</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>true</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>Client Password</Description>
                </Attribute>
                <Attribute>
                    <Name>ClientNonce</Name>
                    <TypeName>String</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>Client Nonce</Description>
                </Attribute>
                <Attribute>
                    <Name>ToConRef</Name>
                    <TypeName>APLink</TypeName>
                    <IsRequired>false</IsRequired>
                    <IsUserAttribute>false</IsUserAttribute>
                    <IsMultiValued>false</IsMultiValued>
                    <DisplayValue>true</DisplayValue>
                    <ValueUnique>true</ValueUnique>
                    <Description>The ConRef leaf indicates the linkage to connectivity parameters.</Description>
                </Attribute>
            </Attributes>
        </ProfileTemplate>

    </ProfileTemplates>
</ProfileMetaData>
