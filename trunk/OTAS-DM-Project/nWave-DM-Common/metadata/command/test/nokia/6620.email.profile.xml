<!-- Nokia 6620 Email Profile command script -->
<Script>
  <Add>
    <InteriorNode>
      <Target>./Email/OTAS1</Target>
    </InteriorNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/Mpro</Target>
      <Data>POP</Data>
    </LeafNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/Mrcv</Target>
      <Data>pop3.otas.cn</Data>
    </LeafNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/Msnd</Target>
      <Data>smtp.otas.cn</Data>
    </LeafNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/Name</Target>
      <Data>MyTestMail01</Data>
    </LeafNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/PW</Target>
      <Data>password</Data>
    </LeafNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/ToNapID</Target>
      <Data>AP/INSG1</Data>
    </LeafNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/UAddr</Target>
      <Data>zhao@iei-china.com</Data>
    </LeafNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/UID</Target>
      <Data>zhaodl</Data>
    </LeafNode>
  </Add>
  <Add>
    <LeafNode>
      <Target>./Email/OTAS1/UName</Target>
      <Data>Zhao DongLu</Data>
    </LeafNode>
  </Add>
</Script>