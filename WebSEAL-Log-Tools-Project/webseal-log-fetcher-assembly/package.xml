<?xml version="1.0" encoding="UTF-8"?>
<assembly>
  <id>bin</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>true</includeBaseDirectory>
  <!-- <baseDirectory>icbc-probe-1.0.0-SNAPSHOT</baseDirectory> -->
  <baseDirectory>webseal-log-tool</baseDirectory>
  <fileSets>
    <!-- Pack Runtime -->
    <fileSet>
      <directory>src/runtime</directory>
      <outputDirectory>/</outputDirectory>
      <includes>
        <include>**</include>
      </includes>
      <fileMode>755</fileMode>
    </fileSet>
  </fileSets>
  <dependencySets>
    <!-- Pack WAR -->
    <dependencySet>
      <outputDirectory>/lib</outputDirectory>
      <unpack>false</unpack>
      <includes>
        <include>*:jar</include>
      </includes>
    </dependencySet>
  </dependencySets>
</assembly>
